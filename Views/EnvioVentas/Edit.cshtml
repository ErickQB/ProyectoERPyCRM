@model ProyectoX.Models.EnvioVenta

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<h4>Envio Venta</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdEnvioVenta" />
            <div class="form-group">
                <label asp-for="IdDespacho" class="control-label"></label>
                <select asp-for="IdDespacho" class="form-control" asp-items="ViewBag.IdDespacho"></select>
                <span asp-validation-for="IdDespacho" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdTransporteEntrega" class="control-label"></label>
                <select asp-for="IdTransporteEntrega" class="form-control" asp-items="ViewBag.IdTransporteEntrega"></select>
                <span asp-validation-for="IdTransporteEntrega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DiaSalida" class="control-label"></label>
                <input asp-for="DiaSalida" class="form-control" />
                <span asp-validation-for="DiaSalida" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HoraSalida" class="control-label"></label>
                <input asp-for="HoraSalida" class="form-control" />
                <span asp-validation-for="HoraSalida" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DiaEntrega" class="control-label"></label>
                <input asp-for="DiaEntrega" class="form-control" />
                <span asp-validation-for="DiaEntrega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HoraEntrega" class="control-label"></label>
                <input asp-for="HoraEntrega" class="form-control" />
                <span asp-validation-for="HoraEntrega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DireccionEntrega" class="control-label"></label>
                <input asp-for="DireccionEntrega" class="form-control" />
                <span asp-validation-for="DireccionEntrega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoEnvio" class="control-label"></label>
                <input asp-for="TipoEnvio" class="form-control" />
                <span asp-validation-for="TipoEnvio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ContactoRecibir" class="control-label"></label>
                <input asp-for="ContactoRecibir" class="form-control" />
                <span asp-validation-for="ContactoRecibir" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="Estado" class="control-label"></label>
                <input asp-for="Estado" class="form-control" />
                <span asp-validation-for="Estado" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="FechaCreacion" class="control-label"></label>
                <input asp-for="FechaCreacion" class="form-control" />
                <span asp-validation-for="FechaCreacion" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="FechaActualizacion" class="control-label"></label>
                <input asp-for="FechaActualizacion" class="form-control" />
                <span asp-validation-for="FechaActualizacion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
